{
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.codeActionsOnSave": [
    "source.organizeImports",
    "source.fixAll.eslint"
  ],
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "cSpell.words": [
    "mantine",
    "MINIO",
    "zincsearch"
  ],
  "i18n-ally.localesPaths": [
    "src/locales"
  ]
}